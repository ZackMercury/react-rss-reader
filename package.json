{
  "name": "rss-reader",
  "version": "1.0.0",
  "description": "A simple RSS Reader, built with PWA standarts, compatible with browsers down to IE9",
  "main": "js/app.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && xcopy \"src/index.html\" \"dist/\" /F /Y && webpack-dev-server --inline",
    "build:production": "webpack -p && xcopy \"src/index.html\" \"dist/\" /F /Y"
  },
  "author": "ZackMercury <suri.the.ange1@live.ru> (http://github.com/ZackMercury)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "nanoajax": "^0.4.3",
    "node-sass": "^4.5.3",
    "react-router-dom": "^4.2.2",
    "sass-loader": "^6.0.6",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  },
  "dependencies": {
    "he": "^1.1.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
}
